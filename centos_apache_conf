Alias /static /home/ebal/mysite/static
<Directory /home/ebal/mysite/static>
    Require all granted
</Directory>

<Directory /home/ebal/mysite/mysite>
    <Files wsgi.py>
        Require all granted
    </Files>
</Directory>


WSGIDaemonProcess mysite python-path=/home/ebal/mysite/:/home/ebal/mysite/venv/lib/python3.6/site-packages
WSGIProcessGroup mysite
WSGIScriptAlias / /home/ebal/mysite/mysite/wsgi.py



sudo usermod -a -G ebal apache
chmod 710 /home/ebal
